{
  "kind": "collectionType",
  "collectionName": "artworks",
  "info": {
    "name": "Artwork"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "status": {
      "type": "enumeration",
      "enum": [
        "draft",
        "published"
      ]
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Objekt",
        "Erlebnis",
        "Motiv"
      ]
    },
    "author": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "generatorShare": {
      "type": "enumeration",
      "enum": [
        "50",
        "100"
      ]
    },
    "price": {
      "type": "decimal"
    },
    "tax": {
      "type": "enumeration",
      "enum": [
        "0",
        "5",
        "16"
      ]
    },
    "shippingCosts": {
      "type": "decimal"
    },
    "email": {
      "type": "email"
    },
    "paypal": {
      "type": "email"
    },
    "quantity": {
      "type": "integer",
      "min": 0
    }
  }
}
