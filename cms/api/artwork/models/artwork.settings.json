{
  "kind": "collectionType",
  "collectionName": "artworks",
  "info": {
    "name": "Artwork"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "type": {
      "type": "enumeration",
      "enum": [
        "Objekt",
        "Erlebnis",
        "Motiv"
      ]
    },
    "author": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "generatorShare": {
      "type": "enumeration",
      "enum": [
        "_50",
        "_100"
      ]
    },
    "payout": {
      "type": "enumeration",
      "enum": [
        "true",
        "false"
      ]
    },
    "price": {
      "type": "decimal"
    },
    "tax": {
      "type": "enumeration",
      "enum": [
        "_0",
        "_5",
        "_16"
      ]
    },
    "shippingCosts": {
      "type": "decimal"
    },
    "email": {
      "type": "email"
    },
    "paypal": {
      "type": "email"
    },
    "quantity": {
      "type": "integer",
      "min": 0
    },
    "street": {
      "type": "string",
      "private": true
    },
    "postalCode": {
      "type": "integer",
      "private": true
    },
    "place": {
      "type": "string",
      "private": true
    },
    "artisticWork": {
      "type": "text",
      "private": true
    },
    "institution": {
      "type": "string",
      "private": true
    },
    "published": {
      "type": "boolean",
      "default": false
    },
    "review": {
      "type": "component",
      "repeatable": false,
      "component": "artist.review"
    },
    "dataRights": {
      "type": "component",
      "repeatable": false,
      "component": "artist.data-rights"
    }
  }
}
