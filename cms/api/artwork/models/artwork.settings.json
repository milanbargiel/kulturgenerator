{
  "kind": "collectionType",
  "collectionName": "artworks",
  "info": {
    "name": "Artwork",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "timestamp": {
      "type": "datetime",
      "private": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Objekt",
        "Erlebnis",
        "Motiv"
      ]
    },
    "author": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "generatorShare": {
      "type": "enumeration",
      "enum": [
        "_50",
        "_100"
      ]
    },
    "payout": {
      "type": "enumeration",
      "enum": [
        "Ja",
        "Nein"
      ],
      "default": "Ja"
    },
    "price": {
      "type": "decimal"
    },
    "tax": {
      "type": "enumeration",
      "enum": [
        "_0",
        "_5",
        "_16"
      ]
    },
    "shippingCosts": {
      "type": "decimal"
    },
    "paypal": {
      "type": "email"
    },
    "quantity": {
      "type": "integer",
      "min": 0,
      "default": 1
    },
    "email": {
      "type": "email"
    },
    "street": {
      "type": "string",
      "private": true
    },
    "postalCode": {
      "type": "integer",
      "private": true
    },
    "place": {
      "type": "string",
      "private": true
    },
    "artisticWork": {
      "type": "text",
      "private": true
    },
    "website": {
      "type": "string"
    },
    "institution": {
      "type": "string",
      "private": true
    },
    "gdpr": {
      "type": "boolean",
      "default": false,
      "private": true
    },
    "imgLicense": {
      "type": "boolean",
      "default": false,
      "private": true
    },
    "vgBild": {
      "type": "boolean",
      "default": false,
      "private": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Eingang",
        "Abgelehnt",
        "Warteliste",
        "ErsteRunde",
        "ZweiteRunde"
      ],
      "default": "Eingang",
      "private": true
    },
    "comment": {
      "type": "text",
      "private": true
    }
  }
}
